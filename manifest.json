{
    "name": "My U2bmemo",
    "description": "Extension for memo to delete halfspace and improve sentence",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["js/halfSpaceDelete.js"],
          "css": ["css/contents.css"]
        }
      ],
      "icons": {
        "48": "u2.png"
      },
    "manifest_version": 3,
    "action": {
      "default_icon": {
        "32": "u2.png"
      },
      "default_popup": "index.html"
    }
}